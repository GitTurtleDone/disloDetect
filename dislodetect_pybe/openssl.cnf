[ req ]
default_bits       = 4096
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[ dn ]
CN = 0.0.0.0

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost
DNS.2 = 0.0.0.0
DNS.3 = localhost:5000
DNS.4 = dislodetect_fe
DNS.5 = dislodetect_fe:3000
DNS.6 = localhost:3000